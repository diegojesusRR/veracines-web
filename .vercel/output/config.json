{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/auth(?:\\/(.*?))?$",
			"dest": "_render"
		},
		{
			"src": "^\\/404\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/forecasts\\/get-forecast-by-combat-id\\.json\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/legal-advice\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/privacy-policy\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/cartelera-([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/$",
			"dest": "_render"
		},
		{
			"src": "/.*",
			"dest": "_render",
			"status": 404
		}
	]
}